{
  "name": "hello.seule",
  "version": "1.0.0",
  "description": "Hello World Project to start with Seule Js Framework",
  "main": "app/splash.html",
  "scripts": {
    "start": "run --x64 --mirror https://dl.nwjs.io/ .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "build --win --x64 --mirror https://npm.taobao.org/mirrors/nwjs/ ."
  },
  "author": "ElML",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElMehdiLebbar/seule-desktop.git"
  },
  "build": {
    "appId": "com.github.ElMehdiLebbar.seule-desktop",
    "nwVersion": "0.58.0",
    "packed": true,
    "targets": [],
    "strippedProperties": [
      "build"
    ],
    "outputPattern": "${NAME} ${VERSION} ${PLATFORM} ${ARCH}",
    "win": {
      "productName": "Hello World",
      "companyName": "Process Skills",
      "copyright": "copyright",
      "icon": "./favicon.ico"
    },
    "mac": {
      "displayName": "Hello World",
      "copyright": "copyright",
      "icon": "./assets/pd.icns",
      "plistStrings": {
        "CFBundleIdentifier": "com.github.ElMehdiLebbar.seule-desktop",
        "CFBundleDocumentTypes": []
      }
    }
  },
  "devDependencies": {
    "nwjs-builder-phoenix": "^1.9.3",
    "typescript": "^2.2.1"
  },
  "window": {
    "title": "Hello World",
    "toolbar": true,
    "frame": false,
    "fullscreen": false,
    "icon": "favicon.png",
    "backgroundColor": "#fefefe",
    "height": 286,
    "width": 600,
    "position": "center",
    "show": false
  },
  "dependencies": {
    "seule": "^1.1.6"
  }
}
